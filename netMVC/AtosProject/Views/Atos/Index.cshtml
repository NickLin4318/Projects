@model IEnumerable<AtosProject.Models.Member>
@{
    ViewBag.Title = "Index";
    var s = ViewBag.xml;
}

<h2>Index</h2>

<div>@s</div>

@using (Html.BeginForm("Add", "Atos", FormMethod.Post))
{
    @Html.AntiForgeryToken()
    <div class="form-group">
    </div>
}

<table class="table">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Gender</th>
    </tr>
    @{
        if (Model.Count() > 0)
        {
            foreach (var item in Model)
            {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Gender</td>
        </tr>
                }
            }
    }
</table>
